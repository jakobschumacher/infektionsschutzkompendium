{
  "name": "infektionsschutzkompendium",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "postbuild": "react-snap",
    "preview": "vite preview",
    "extract": "python3 extract_data.py",
    "deploy": "npm run build && gh-pages -d dist"
  },
  "reactSnap": {
    "source": "dist",
    "inlineCss": true,
    "puppeteerArgs": [
      "--no-sandbox",
      "--disable-setuid-sandbox"
    ],
    "include": [
      "/",
      "/about"
    ]
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.28.0"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^4.3.4",
    "gh-pages": "^6.3.0",
    "react-snap": "^1.23.0",
    "vite": "^6.0.1"
  }
}
